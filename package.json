{
  "name": "ng-snotify",
  "description": "Angular 2 notifications center",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "example:build": "ng build --base-href=/ng-snotify/  --output-path=docs/",
    "example:build:prod": "ng build -prod --base-href=/ng-snotify/  --output-path=docs/",
    "example:test": "ng test",
    "example:lint": "ng lint",
    "e2e": "ng e2e",
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.docs.json -n ng-snotify -d -d docs/documentation/ --hideGenerator --theme Laravel",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check -p src/tsconfig.es5.json src/**/*.ts",
    "test": "tsc && karma start",
    "app:build": "npm run build && npm run example:build:prod && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://artemsky.github.io/ng-snotify"
  },
  "author": {
    "name": "artemsky",
    "email": "mr.artemsky@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 2",
    "angular 4",
    "Angular2",
    "ng2",
    "ng4",
    "Angular4",
    "4",
    "2",
    "Library",
    "Notifications",
    "Notification",
    "Toast",
    "toasts",
    "promt",
    "async",
    "confirmation",
    "notify",
    "notie",
    "notification-center",
    "snotify",
    "ng-snotify",
    "ng2-snotify"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://artemsky.github.io/ng-snotify/issues"
  },
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/cli": "1.1.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.7",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "bootstrap": "^3.3.7",
    "codelyzer": "~3.0.1",
    "core-js": "^2.4.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.11.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.2",
    "rollup": "^0.41.6",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "ts-node": "~3.0.4",
